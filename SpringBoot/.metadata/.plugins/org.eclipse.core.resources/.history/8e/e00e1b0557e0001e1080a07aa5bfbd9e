package com.yakrut.AddressService.repo;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.query.Param;

public interface AddressRepo extends JpaRepository<Address, Integer>{
	@Query(value="select ea.id,ea.lane1,ea.lane2,ea.state,ea.zip from address ea join employee e on ea.employee_id = e.id where e.id=1")
	findAddressByEmployeeId(@Param("employeeId") String employeeId)
}
