	<servlet>  
		<servlet-name>s1</servlet-name>  
		<servlet-class>com.java.servlets.attribute.AttributeServlet1</servlet-class>  
	</servlet>  
	  
	<servlet-mapping>  
		<servlet-name>s1</servlet-name>  
		<url-pattern>/AttributeServlet1</url-pattern>  
	</servlet-mapping>  
	  
	<servlet>  
		<servlet-name>s2</servlet-name>  
		<servlet-class>com.java.servlets.attribute.AttributeServlet2</servlet-class>  
	</servlet>  
	  
	<servlet-mapping>  
		<servlet-name>s2</servlet-name>  
